import requests
import json

headers = {
    'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36',
    'Accept-Language': 'en-US,en;q=0.9',
    'Accept': 'application/json, text/plain, */*'
}

def check_endpoint(url):
    print(f"Checking URL: {url}")
    try:
        response = requests.get(url, headers=headers)
        print(f"Status Code: {response.status_code}")
        if response.status_code == 200:
            data = response.json()
            # specific to nasdaq response structure often being wrapped in 'data'
            if 'data' in data:
                print("Keys in data:", data['data'].keys())
                # print a snippet
                print(json.dumps(data['data'], indent=2)[:500])
            else:
                print("Response JSON (first 500 chars):", json.dumps(data, indent=2)[:500])
        else:
            print("Response:", response.text[:200])
    except Exception as e:
        print(f"Error: {e}")

# Try the financials endpoint
check_endpoint('https://api.nasdaq.com/api/company/AAPL/financials?frequency=1')
# Try looking for ratiostable if possible or just the main one
check_endpoint('https://api.nasdaq.com/api/company/AAPL/financials?frequency=1&frequency=1')
